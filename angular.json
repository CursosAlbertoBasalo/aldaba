{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "www",
  "schematics": {
    "@nrwl/angular": {},
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "enableIvy": true,
      "inlineStyle": true,
      "prefix": "ab",
      "routing": true,
      "skipInstall": true,
      "skipTests": true,
      "strict": true,
      "tags": "app"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true,
      "directory": "domain",
      "enableIvy": true,
      "inlineStyle": true,
      "prefix": "ab",
      "simpleModuleName": true,
      "skipInstall": true,
      "skipTests": true,
      "strict": true,
      "tags": "lib"
    },
    "@nrwl/angular:component": {
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "skipTests": true,
      "style": "css"
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "skipTests": true,
      "style": "css"
    },
    "@nrwl/angular:service": {
      "skipTests": true,
      "flat": true
    },
    "@nrwl/angular:interceptor": {
      "skipTests": true,
      "flat": true
    },
    "@schematics/angular:interceptor": {
      "skipTests": true,
      "flat": true
    },
    "@nrwl/angular:guard": {
      "skipTests": true,
      "flat": true
    },
    "@schematics/angular:guard": {
      "skipTests": true,
      "flat": true
    },
    "@nrwl/angular:directive": {
      "skipTests": true,
      "flat": true
    },
    "@schematics/angular:directive": {
      "skipTests": true,
      "flat": true
    },
    "@nrwl/angular:pipe": {
      "skipTests": false,
      "flat": true
    },
    "@schematics/angular:pipe": {
      "skipTests": false,
      "flat": true
    },
    "@schematics/angular:service": {
      "skipTests": true,
      "flat": true
    }
  },
  "projects": {
    "www": {
      "projectType": "application",
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "prefix": "ab",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/www",
            "index": "apps/www/src/index.html",
            "main": "apps/www/src/main.ts",
            "polyfills": "apps/www/src/polyfills.ts",
            "tsConfig": "apps/www/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/www/src/favicon.ico", "apps/www/src/assets"],
            "styles": ["apps/www/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "www:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "www:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "www:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/www/src/**/*.ts",
              "apps/www/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/www"],
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "tsConfig": "apps/www-e2e/tsconfig.e2e.json",
            "devServerTarget": "www:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/www-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-global": {
      "projectType": "library",
      "root": "libs/shared/global",
      "sourceRoot": "libs/shared/global/src",
      "prefix": "ab",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/shared/global/tsconfig.lib.json",
            "project": "libs/shared/global/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/global/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/global/src/**/*.ts",
              "libs/shared/global/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/global"],
          "options": {
            "jestConfig": "libs/shared/global/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-data": {
      "projectType": "library",
      "root": "libs/shared/data",
      "sourceRoot": "libs/shared/data/src",
      "prefix": "ab",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/shared/data/tsconfig.lib.json",
            "project": "libs/shared/data/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/data/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/data/src/**/*.ts",
              "libs/shared/data/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/data"],
          "options": {
            "jestConfig": "libs/shared/data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
